    style.
            .row + .row {
            margin-top: 15px;
            }
            .container {
            margin: 30px auto;
            }
            .bg-primary {
            background-color: #78C2AD !important;
            }
            .border-primary { border-color: #78C2AD !important; }
            .form-control:focus {
            border-color: #d0eae2;
            box-shadow: 0 0 0 0.2rem rgba(120, 194, 173, 0.25);
            }
            .btn-primary {
            background-color: #78C2AD;
            border-color: #78C2AD;
            outline: none;
            }
            .btn-primary:hover,
            .btn-primary:not(:disabled):not(.disabled):active {
            background-color: #5eb69d;
            border-color: #55b298;
            outline: none;
            }
            .btn-primary:focus {
            box-shadow: none;
            box-shadow: 0 0 0 0.2rem rgba(120, 194, 173, 0.5);
            }
            .text-primary {
            color: #78C2AD !important;
            }
